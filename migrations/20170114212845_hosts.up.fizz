create_table("hosts", func(t) {
	t.Column("id", "uuid", {"primary": true})
	t.Column("name", "string")
	t.Column("description", "string")
	t.Column("url", "string")
	t.Column("api_version", "string")
	t.Column("access_key", "string")
	t.Column("secret_key", "string")
	t.Column("created_at","time.Time")
	t.Column("updated_at","time.Time")
})